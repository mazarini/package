{
    "name": "mazarini/package",
    "type": "symfony-bundle",
    "description": "List the installed packages, the required packages and the reasons for installing the packages.",
    "keywords": [
        "symfony",
        "bundle",
        "command",
        "package"
    ],
    "homepage": "https://github.com/mazarini/package",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4.11|^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.11",
        "symfony/framework-bundle": "^5.2",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mazarini\\PackageBundle\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {},
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "minimum-stability": "stable"
}
